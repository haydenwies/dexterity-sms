{
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": false,
	"quoteProps": "preserve",
	"jsxSingleQuote": false,
	"trailingComma": "none",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"proseWrap": "never",
	"endOfLine": "lf",
	"singleAttributePerLine": true,
	"plugins": ["prettier-plugin-tailwindcss"]
}
